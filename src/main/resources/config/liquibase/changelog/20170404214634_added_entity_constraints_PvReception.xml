        <addForeignKeyConstraint baseColumnNames="superviseur_id"
                                 baseTableName="pv_reception"
                                 constraintName="fk_pv_reception_superviseur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employe"/>

    </changeSet>
</databaseChangeLog>
